@model CarStoreWebApp.Models.AboutModel

<form asp-action="Add"asp-controller="About"asp-antiforgery="true"enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="form-group">
            <label asp-for="Adress">Адресс</label>
            <input type="text" asp-for="Adress" class="form-control" />
            <span asp-validation-for="Adress"></span>
        </div>
        <div class="form-group">
            <label asp-for="Email">Почта</label>
            <input type="text" asp-for="Email" class="form-control" />
            <span asp-validation-for="Email"></span>
        </div>
        <div class="form-group">
            <label asp-for="Phone">Номер телефона</label>
            <input type="number" asp-for="Phone" class="form-control" />
            <span asp-validation-for="Phone"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Добавить" class="form-control btn" />
        </div>
    </div>

</form>

